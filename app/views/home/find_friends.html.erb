<div class="row">
  <div class="col-lg-3">
    <%= render 'includes/user_info' %>
  </div>
  <div class="col-lg-6 well">
    <h3>Find your friends</h3>
    <hr>
    <div id="users">
      <%= render(@users) || render('includes/no_resources', resources: 'Users') %>
    </div>
    <%= render 'includes/paginate', resources: @users %>
  </div>
  <div class="col-lg-3">
    <%= render 'includes/links' %>
  </div>
</div>
